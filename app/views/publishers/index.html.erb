<h1>Publisher</h1>

<ul>
  <% @publishers.each do |publisher| %>
    <li>
      <%= link_to publisher.name, publisher_path(publisher) %>
    </li>
  <% end %>
</ul>

<div>
  <%= button_to "Create New Publisher", new_publisher_path, method: :get, class: 'btn-primary' %>
  <%= button_to 'Back to Home', root_path, method: :get, class: 'btn' %>
</div>
